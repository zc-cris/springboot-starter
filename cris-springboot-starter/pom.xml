        -->
        <dependency>
            <groupId>com.cris.starter</groupId>
            <artifactId>cris-springboot-stater-autoconfigurer</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>

    </dependencies>

    
</project>